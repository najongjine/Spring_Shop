<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.biz.tour.dao.member.MemberDao">

	<insert id="insert" parameterType="memberVO">
		INSERT INTO
		`tbl_members`
		(
		`u_name`,
		`u_password`,
		`u_role`,
		`email`,
		`phone`,
		`address`,
		`enabled`)
		VALUES
		(
		#{u_name},
		#{u_password},
		#{u_role},
		#{email},
		#{phone},
		#{address},
		#{enabled}
		)


	</insert>

</mapper> 