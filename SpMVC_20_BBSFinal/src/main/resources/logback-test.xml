<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<!--  console 에 log를 표시할때 사용할 패턴 설정 -->
	<appender name="myConsole" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<!-- logback level
	ERROR
	WARN
	INFO 
	DEBUG
	TRACE
	만약 level INFO로 설정했다
	INFO 이하의 메세지는 모두 무시  -->
	
	<root level="INFO">
		<appender-ref ref="myConsole"/>
	</root>
</configuration>